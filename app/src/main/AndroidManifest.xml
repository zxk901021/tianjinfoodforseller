<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.zhy_9.tianjinfoodgroup">

    <uses-permission android:name="android.permission.INTERNET" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity
            android:name=".LoginActivity"
            android:launchMode="singleTask">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".SellerManagerActivity"
            android:label="@string/title_activity_seller_manager"
            android:theme="@style/AppTheme" />
        <activity android:name=".ForgetPasswordActivity" />
        <activity android:name=".OrderListActivity" />
        <activity android:name=".GoodsActivity" />
        <activity android:name=".OnSellingGoodsActivity" />
        <activity android:name=".AccountSecurityActivity" />
        <activity android:name=".MallMessageActivity" />
        <activity android:name=".SetPasswordActivity"></activity>
    </application>

</manifest>